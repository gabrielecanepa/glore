{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.json"],
  "globalEnv": [
    "GH_TOKEN",
    "GITHUB_TOKEN",
    "ISSUE_PREFIX",
    "ISSUE_URL",
    "NODE_ENV",
    "OPENAI_API_KEY",
    "SUPABASE_ACCESS_TOKEN",
    "SUPABASE_DB_URL"
  ],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", ".vercel/output/**", "build/**"],
      "env": [
        "ENABLE_EXPERIMENTAL_COREPACK",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_PASSWORD",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_USER",
        "SUPABASE_ANON_KEY",
        "SUPABASE_JWT_SECRET",
        "SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_URL"
      ]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true,
      "env": ["SUPABASE_ANON_KEY", "SUPABASE_URL"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "watch": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": ["**/coverage/**"]
    },
    "typegen": {
      "outputs": ["**/*.d.ts"]
    },
    "type-check": {
      "outputs": ["**/tsconfig.tsbuildinfo"]
    },
    "db": {
      "dependsOn": ["^db"],
      "outputs": ["supabase/.branches/**", "supabase/.temp/**", "supabase/types.ts"],
      "env": ["SUPABASE_DB_URL", "SUPABASE_SERVICE_ROLE_KEY"]
    },
    "lint": {},
    "format": {},
    "check": {}
  }
}

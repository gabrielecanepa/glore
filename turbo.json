{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["ANALYZE", "NODE_ENV"],
  "tasks": {
    "dev": {
      "cache": false,
      "interactive": true,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "build/**"],
      "env": [
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_PASSWORD",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_URL",
        "POSTGRES_USER",
        "SUPABASE_ANON_KEY",
        "SUPABASE_JWT_SECRET",
        "SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_URL"
      ]
    },
    "build:watch": {
      "cache": false,
      "interactive": true,
      "outputs": ["build/**"],
      "persistent": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "typegen": {
      "dependsOn": ["^typegen"],
      "outputs": ["supabase/**"],
      "env": ["SUPABASE_URL"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "check:fix": {
      "dependsOn": ["^check:fix"]
    }
  }
}

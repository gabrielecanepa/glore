{
  "name": "elearning",
  "version": "0.4.3",
  "type": "module",
  "scripts": {
    "prepare": "sh .bin/prepare",
    "build": "sh .bin/typegen && next build",
    "start": "next start",
    "dev": "sh .bin/dev",
    "db:start": "supabase db start",
    "db:stop": "supabase stop",
    "db:dump": "sh .bin/db:dump",
    "db:pull": "sh .bin/db:pull",
    "db:push": "supabase db push",
    "db:migrate": "supabase migration up",
    "db:seed": "tsx supabase/seed.ts",
    "db:sync": "sh .bin/typegen && snaplet-seed sync",
    "db:reset": "supabase db reset && tsx supabase/seed.ts",
    "typegen": "sh .bin/typegen",
    "type-check": "tsc -p tsconfig.build.json",
    "lint": "eslint src && prettier --check . && shellcheck .bin/*",
    "format": "prettier --write . && eslint --fix .",
    "check": "pnpm type-check && pnpm lint",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@repo/utils": "workspace:^",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/canvas-confetti": "^1.9.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.522.0",
    "next": "^15.3.4",
    "next-intl": "^4.3.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.4",
    "@repo/eslint-config": "workspace:^",
    "@repo/prettier-config": "workspace:^",
    "@repo/tsconfig": "workspace:^",
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "0.98.0",
    "@supabase/postgrest-js": "^1.19.4",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "catalog:",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "dotenv": "^16.5.0",
    "eslint": "catalog:",
    "openai": "^5.7.0",
    "pg": "^8.16.2",
    "postcss": "^8.5.6",
    "postgres": "^3.4.7",
    "prettier": "catalog:",
    "prettier-plugin-sql": "catalog:",
    "react-scan": "^0.3.4",
    "shadcn": "^2.7.0",
    "shellcheck": "catalog:",
    "supabase": "^2.26.9",
    "tailwindcss": "^4.1.10",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier-config/sql",
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {}
    }
  },
  "@snaplet/seed": {
    "config": "supabase/seed.config.ts"
  }
}
